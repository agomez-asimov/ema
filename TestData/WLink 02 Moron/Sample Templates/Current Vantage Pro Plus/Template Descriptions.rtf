\par {\pntext\pard\plain\f3\insrsid3954220 \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard \ql \fi-353\li540\ri130\sb120\sa60\nowidctlpar{\*\pn \pnlvlbody\ilvl0\ls1\pnrnot0\pnf3\pnindent360 {\pntxtb \'b7}}\faauto\ls1\rin130\lin540\itap0 {\insrsid3954220 
